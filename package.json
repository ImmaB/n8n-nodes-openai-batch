{
  "name": "n8n-nodes-openai-batch",
  "version": "1.0.0",
  "description": "n8n node for OpenAI Batch API requests",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "openai",
    "batch",
    "ai"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": ""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build",
    "test": "npx ts-node test/test-batch.ts"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OpenAiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/OpenAiBatch/OpenAiBatch.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "dotenv": "^16.3.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.22.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
